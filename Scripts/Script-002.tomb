struct my_rom_data {
	name:string;
	counter:number;
}

script token_minter {

	import Token;
	import NFT;

	code(source:address, target:address) {
		local rom_data:my_rom_data = Struct.my_rom_data("hello", 123);
		local seriesID : number = 1;
		NFT.mint(source, target, "LOL", rom_data, "ram_can_be_anything", seriesID);
	}
}